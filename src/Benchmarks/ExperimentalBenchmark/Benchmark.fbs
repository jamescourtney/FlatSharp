namespace BenchmarkCore;

struct Foo {
  id:ulong;
  count:short;
  prefix:byte;
  length:uint;
}

struct Bar {
  parent:Foo;
  time:int;
  ratio:float;
  size:ushort;
}

table FooBar (fs_serializer) {
  sibling:Bar (required);
  name:string (required);
  rating:double;
  postfix:ubyte;
}

table FooBarContainer (fs_serializer) {
  list:[FooBar] (required);  // 3 copies of the above
  initialized:bool;
  fruit:short;
  location:string (required);
}
