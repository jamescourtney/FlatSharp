namespace Samples.StructMarshalling;

table Table (fs_serializer:Lazy)
{
	         Vectors : [Vector3];
	IrregularStructs : [IrregularStruct];
}

// Conforms to the format of System.Numerics.Vector3.
struct Vector3 
{
    X : float;
	Y : float;
	Z : float;
}

// This struct in FlatBuffers is only 9 bytes.
struct IrregularStruct
{
    A : ulong;
	B : byte;
}